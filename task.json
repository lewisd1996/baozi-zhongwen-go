{
    "executionRoleArn": "arn:aws:iam::450706242431:role/BZ-AppRole",
    "containerDefinitions": [
        {
            "name": "BZ-Container",
            "image": "$REPOSITORY_URI:$IMAGE_TAG",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 3000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "BZ-InfraStackTaskDef"
}